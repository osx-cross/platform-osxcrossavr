{
  "name": "osxcrossavr",
  "title": "Latest AVR Toolchain",
  "description": "Latest AVR toolchain brought to you by osx-cross to use the latest version of gcc, binutils and avrlibc.",
  "url": "https://github.com/osx-cross/platform-osxcrossavr",
  "homepage": "https://github.com/osx-cross/platform-osxcrossavr",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
	"url": "https://github.com/osx-cross/platform-osxcrossavr.git"
  },
  "version": "1.8.1",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json",
    "https://raw.githubusercontent.com/eerimoq/simba/master/make/platformio/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-arduinoavr",
      "script": "builder/frameworks/arduino.py"
    },
    "simba": {
      "package": "framework-simba",
      "script": "builder/frameworks/simba.py"
    }
  },
  "packages": {
      "framework-arduinoavr": {
      "type": "framework",
      "optional": true,
      "version": "~1.10620.2"
    },
    "framework-simba": {
      "type": "framework",
      "optional": true,
      "version": ">=7.0.0"
    },
    "tool-avrdude": {
      "type": "uploader",
      "optional": true,
      "version": "~1.60300.0"
    },
    "tool-micronucleus": {
      "type": "uploader",
      "optional": true,
      "version": "~1.200.0"
    }
  }
}
